<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kkk.op.user.persistence.mapper.UserMapper">

  <select id="listByGender" resultType="com.kkk.op.user.persistence.UserDO">
    SELECT *
    FROM user
    WHERE gender = #{gender}
  </select>

</mapper>